<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox id="_AdminQuizRightSidebar" alignment="CENTER" onMouseClicked="#handleAppMouseClick_Hold" onMouseDragged="#handleAppMouseDrag" prefHeight="475.0" prefWidth="150.0" xmlns:fx="http://javafx.com/fxml" fx:controller="com.sohail.online_quizzing_app.controller._AdminQuizRightSidebarController">
  <children>
    <Label fx:id="total_q_added_label" text="Total Questions Added :" VBox.vgrow="SOMETIMES">
      <VBox.margin>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </VBox.margin>
    </Label>
    <TextField id="txtbox_adminAddQuestionTotalQuestionAddedd" fx:id="total_q_added" editable="false" prefHeight="30.0" prefWidth="200.0" VBox.vgrow="SOMETIMES">
      <VBox.margin>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </VBox.margin>
    </TextField>
    <Label text="Select Question Number: " VBox.vgrow="SOMETIMES">
      <VBox.margin>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </VBox.margin>
    </Label>
    <ComboBox id="combo_adminSelectQNo" fx:id="select_q" disable="true" mouseTransparent="false" prefHeight="30.0" prefWidth="200.0" promptText="Click Me" VBox.vgrow="SOMETIMES">
      <items>
        <FXCollections fx:factory="observableArrayList">
          <String fx:value="Item 1" />
          <String fx:value="Item 2" />
          <String fx:value="Item 3" />
        </FXCollections>
      </items>
      <VBox.margin>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </VBox.margin>
    </ComboBox>
    <Button id="btn_adminDeleteQ" fx:id="delete" cancelButton="true" disable="true" mnemonicParsing="false" prefHeight="40.0" prefWidth="140.0" text="Delete">
      <VBox.margin>
        <Insets bottom="5.0" left="5.0" right="5.0" top="30.0" />
      </VBox.margin>
    </Button>
    <Button id="btn_adminPreviousQ" fx:id="previous" disable="true" mnemonicParsing="false" prefHeight="40.0" prefWidth="140.0" text="Previous">
      <VBox.margin>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </VBox.margin>
    </Button>
    <Button id="btn_adminManageQuiz" fx:id="next" disable="true" mnemonicParsing="false" prefHeight="40.0" prefWidth="140.0" text="Next">
      <VBox.margin>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </VBox.margin>
    </Button>
    <Button id="btn_adminViewQuiz" fx:id="update" disable="true" mnemonicParsing="false" prefHeight="40.0" prefWidth="140.0" text="Update">
      <VBox.margin>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </VBox.margin>
    </Button>
    <Button id="save_all_options" fx:id="createNextQuestion" mnemonicParsing="false" onAction="#createNextQuestion" prefHeight="40.0" prefWidth="140.0" text="Next Question" VBox.vgrow="SOMETIMES">
      <VBox.margin>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </VBox.margin>
    </Button>
    <Button id="btn_adminUpdateQ" fx:id="publish_quiz" defaultButton="true" mnemonicParsing="false" onAction="#publishQuiz" prefHeight="40.0" prefWidth="140.0" text="Publish Quiz" VBox.vgrow="SOMETIMES">
      <VBox.margin>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </VBox.margin>
    </Button>
  </children>
  <padding>
    <Insets bottom="10.0" top="10.0" />
  </padding>
  <stylesheets>
    <URL value="@styles/_AdminQuizRightSidebar.css" />
  </stylesheets>
</VBox>
