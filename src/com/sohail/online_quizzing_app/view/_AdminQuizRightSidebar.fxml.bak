<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox id="_AdminQuizRightSidebar" alignment="CENTER" prefHeight="475.0" prefWidth="150.0" xmlns:fx="http://javafx.com/fxml" fx:controller="com.sohail.online_quizzing_app.controller._AdminQuizRightSidebarController">
    <children>
        <Label text="Total Questions Added :" VBox.vgrow="SOMETIMES">
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </VBox.margin>
        </Label>
        <TextField id="txtbox_adminAddQuestionTotalQuestionAddedd" fx:id="txtbox_adminTotalQAdded" editable="false" prefHeight="30.0" prefWidth="200.0" VBox.vgrow="SOMETIMES">
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </VBox.margin>
        </TextField>
        <Label text="Select Question Number: " VBox.vgrow="SOMETIMES">
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </VBox.margin>
        </Label>
        <ComboBox fx:id="combo_adminSelectQNo" mouseTransparent="false" prefHeight="30.0" prefWidth="200.0" promptText="Click Me" VBox.vgrow="SOMETIMES">
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:value="Item 1" />
                    <String fx:value="Item 2" />
                    <String fx:value="Item 3" />
                </FXCollections>
            </items>
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </VBox.margin>
        </ComboBox>
        <Button fx:id="btn_adminDeleteQ" cancelButton="true" disable="false" mnemonicParsing="false" prefHeight="40.0" prefWidth="140.0" text="Delete">
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="15.0" />
            </VBox.margin>
        </Button>
        <Button fx:id="btn_adminPreviousQ" disable="false" mnemonicParsing="false" prefHeight="40.0" prefWidth="140.0" text="Previous">
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </VBox.margin>
        </Button>
        <Button id="btn_adminManageQuiz" fx:id="btn_adminNextQ" disable="false" mnemonicParsing="false" prefHeight="40.0" prefWidth="140.0" text="Next">
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </VBox.margin>
        </Button>
        <Button id="btn_adminViewQuiz" fx:id="btn_adminUpdateQ" mnemonicParsing="false" prefHeight="40.0" prefWidth="140.0" text="Update">
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </VBox.margin>
        </Button>
        <Button id="btn_adminUpdateQ" fx:id="btn_adminPublishQuiz" defaultButton="true" mnemonicParsing="false" prefHeight="40.0" prefWidth="140.0" text="Publish Quiz" VBox.vgrow="SOMETIMES">
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </VBox.margin>
        </Button>
    </children>
    <padding>
        <Insets bottom="10.0" top="10.0" />
    </padding>
    <stylesheets>
        <URL value="@styles/_AdminQuizRightSidebar.css" />
    </stylesheets>
</VBox>
