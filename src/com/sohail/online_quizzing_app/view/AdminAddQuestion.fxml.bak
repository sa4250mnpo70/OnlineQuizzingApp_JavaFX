<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<BorderPane id="AdminAddQuestion" prefHeight="600.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml" fx:controller="com.sohail.online_quizzing_app.controller.AdminAddQuestionController">
    <bottom>
        <fx:include source="_AppFooter.fxml" />
    </bottom>
    <center>
        <GridPane BorderPane.margin="$x1">
            <children>
                <VBox alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="0" GridPane.columnSpan="7" GridPane.hgrow="SOMETIMES" GridPane.margin="$x1" GridPane.rowIndex="0" GridPane.rowSpan="7" GridPane.vgrow="SOMETIMES">
                    <children>
                        <VBox id="VBox" alignment="CENTER_LEFT" spacing="5.0" VBox.margin="$x1" VBox.vgrow="SOMETIMES">
                            <children>
                                <Label text="Type Your Question Below : " VBox.vgrow="SOMETIMES">
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" fx:id="x1" />
                                    </VBox.margin>
                                </Label>
                                <TextArea prefWidth="-1.0" promptText="This is a sample question." wrapText="true" VBox.margin="$x1" VBox.vgrow="SOMETIMES" />
                            </children>
                        </VBox>
                        <HBox alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0" VBox.margin="$x1">
                            <children>
                                <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="-1.0" text="Click Me To Select an Image for the Question (Optional) " HBox.hgrow="SOMETIMES" HBox.margin="$x1" />
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0" VBox.margin="$x1">
                            <children>
                                <ComboBox prefHeight="30.0" prefWidth="320.0" promptText="Select Difficulty Level for this question " HBox.hgrow="SOMETIMES" HBox.margin="$x1">
                                    <items>
                                        <FXCollections fx:factory="observableArrayList">
                                            <String fx:value="Item 1" />
                                            <String fx:value="Item 2" />
                                            <String fx:value="Item 3" />
                                        </FXCollections>
                                    </items>
                                </ComboBox>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0" VBox.margin="$x1">
                            <children>
                                <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="150.0" text="Cancel" HBox.hgrow="SOMETIMES" HBox.margin="$x1" />
                                <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="150.0" text="Clear" HBox.hgrow="SOMETIMES" HBox.margin="$x1" />
                                <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="150.0" text="Save" HBox.hgrow="SOMETIMES" HBox.margin="$x1" />
                            </children>
                        </HBox>
                    </children>
                </VBox>
            </children>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
        </GridPane>
    </center>
    <left>
        <fx:include source="_AppAdminSideBar.fxml" />
    </left>
    <right>
        <fx:include source="_AdminQuizRightSidebar.fxml" />
    </right>
    <stylesheets>
        <URL value="@styles/AdminAddQuestion.css" />
    </stylesheets>
    <top>
        <fx:include source="_AppHeader.fxml" />
    </top>
</BorderPane>
